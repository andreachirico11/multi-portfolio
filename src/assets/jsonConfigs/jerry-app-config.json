{
  "appId": "GerryApp",
  "rootComponentConfig": {
    "navbar1": {
      "componentId": "navbar_1",
      "outletId": "navbar_1_slot"
    },
    "navbar2": {
      "componentId": "navbar_2",
      "outletId": "navbar_2_slot"
    }
  },
  "routes": [
    {
      "path": "",
      "emptyRoute": true
    },
    {
      "title": "about",
      "path": "about",
      "lazy": true,
      "componentId": "paragraph_list_1",
      "componentType": "PARAGRAPH_LIST"
    },
    {
      "title": "contact",
      "path": "contact",
      "lazy": true,
      "componentId": "contact_1",
      "componentType": "FORM"
    },
    {
      "title": "vimeo",
      "path": "vimeo",
      "lazy": true,
      "componentId": "vimeo_1",
      "componentType": "VIMEO"
    },
    {
      "title": "releases",
      "path": "releases",
      "lazy": true,
      "componentId": "card_slideshow_1",
      "componentType": "CARD_SLIDESHOW"
    },
    {
      "title": "papers",
      "path": "papers",
      "lazy": true,
      "componentId": "paragraph_list_2",
      "componentType": "PARAGRAPH_LIST"
    },
    {
      "title": "projects",
      "path": "projects",
      "lazy": true,
      "componentId": "custom_proj_nav",
      "componentType": "CUSTOM_PROJ_NAV"
    },
    {
      "title": "projects",
      "path": "projects/:projectName",
      "lazy": true,
      "componentId": "project_card",
      "componentType": "CARD",
      "pathParameters": [
        "projectName"
      ]
    },
    {
      "title": "lessons",
      "path": "lessons",
      "lazy": true,
      "componentId": "card_slideshow_2",
      "componentType": "CARD_SLIDESHOW"
    },
    {
      "title": "live",
      "path": "live",
      "lazy": true,
      "componentId": "live_nav",
      "componentType": "NAVBAR"
    },
    {
      "title": "live",
      "path": "live/:year",
      "lazy": true,
      "componentId": "paragraph_list_live",
      "componentType": "PARAGRAPH_LIST",
      "pathParameters": [
        "year"
      ]
    },
    {
      "componentId": "404_component",
      "componentType": "404",
      "lazy": false,
      "title": "error",
      "path": "**"
    }
  ]
}
